{
    "name": "azozzalfiras/aapanel-api",
    "description": "A PHP client library for interacting with the aapanel API",
    "require": {
        "php": "^7.2",
        "azozzalfiras/aapanel-api": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "authors": [
        {
            "name": "Azozz ALFiras",
            "email": "azozzalfiras@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "AzozzALFiras\\AAPanelAPI\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/AzozzALFiras/aapanel-api"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true
}
